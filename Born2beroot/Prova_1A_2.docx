<!-- Script que retira os cursos "Sala de Aula de ..." do menu lateral "Minhas Salas"
-->
<script type="text/javascript">
function removeSalasMultimidiaDoMenu() {
    let li_collection = document.querySelectorAll("#nav-drawer ul>li");
    
    for (let i = 0; i < li_collection.length; i++) {
        let li_elem = li_collection[i];
        let child = li_elem.querySelector("[data-parent-key=mycourses] .ml-1 .media>.media-body");
        
        if (child && child.textContent.startsWith("Sala de Aula de")) {
            li_elem.parentNode.removeChild(li_elem);
        }
    }
}

function removeSalasMultimidiaDoBlocoMinhasSalas() {
    let li_collection = document.querySelectorAll("#frontpage-course-list>.frontpage-course-list-enrolled .coursebox");
    
    for (let i = 0; i < li_collection.length; i++) {
        let div_elem = li_collection[i];
        let child = div_elem.querySelector(".aalink");
        
        if (child && child.textContent.startsWith("Sala de Aula de")) {
            div_elem.parentNode.removeChild(div_elem);
        }
    }
}

document.addEventListener("DOMContentLoaded", function(event) { 
    removeSalasMultimidiaDoMenu();
    removeSalasMultimidiaDoBlocoMinhasSalas();
});
</script>

